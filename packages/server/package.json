    "main": "dist/index.js",
    "type": "module",
    "directories": {
        "test": "tests"
    },
    "files": [
        "lib"
    ],
    "scripts": {
        "build:ci": "yarn tsc",
        "build:dist": "./bin/dist.sh",
        "build:docker": "./bin/docker.sh --image",
        "build:next": "./bin/next.sh",
        "build:test": "echo build:test",
        "build:watch": "yarn tsc --watch",
        "build": "yarn tsc",
        "ci": "yarn install --immutable",
        "clean": "rm -rf dist",
        "debug:loop": "while true; do yarn debug; done",
        "debug": "node --inspect-brk ./dist/index.js --no-watch",
        "dev": "nodemon dist/index.js",
        "docs": "jsdoc system -r -c .jsdocrc.json -d docs",
        "migrate:deploy": "yarn prisma migrate deploy",
        "migrate:dev": "yarn prisma migrate dev",
        "migrate:reset": "yarn prisma migrate reset",
        "postbuild:ci": "rm -rf src",
        "prebuild:ci": "yarn clean",
        "prestart:docker": "build:docker",
        "pretest:ci": "yarn build",
        "seed": "node dist/index.js --seed",
        "start:docker": "./bin/docker.sh --container",
        "start": "node dist/index.js --serve",
        "test:ci": "yarn test",
        "test:push": "yarn build && yarn test && yarn semantic-release",
        "test:watch": "yarn node --experimental-vm-modules --no-warnings=ExperimentalWarning $(yarn bin jest)  --detectOpenHandles --watch",
        "test": "yarn node --experimental-vm-modules --no-warnings=ExperimentalWarning $(yarn bin jest) --detectOpenHandles"
    },
