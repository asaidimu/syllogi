        "jest-mock-extended": "^3.0.3",
        "node-notifier": "^10.0.1",
        "nodemon": "^2.0.20",
        "prettier": "^2.3.2",
        "prisma": "latest",
        "supertest": "^6.2.4",
        "typescript": "^4.9.4"
    },
