    "scripts": {
        "tsc": "tsc",
        "start": "node dist/index.js",
        "dev": "nodemon dist/index.js",
        "docs": "jsdoc system -r -c .jsdocrc.json -d docs",
        "prebuild": "yarn clean",
        "build": "yarn tsc",
        "postbuild": "rm -rf src",
        "build:watch": "yarn tsc --watch",
        "ci": "yarn install --immutable",
        "clean": "rm -rf dist",
        "debug": "node --inspect-brk ./dist/index.js --no-watch",
        "debug:loop": "while true; do yarn debug; done",
        "pretest:ci": "yarn build",
        "test": "alcides",
        "test:ci": "yarn test",
        "test:push": "yarn build && yarn test && yarn semantic-release",
        "build:test": "echo build:test",
        "build:docker": "cd ../../ && yarn docker-ctx -p '@syllogi/server' -c packages/server/Dockerfile | docker build --build-arg PACKAGE_NAME='@syllogi/server' - -t syllogi-server"
    },
