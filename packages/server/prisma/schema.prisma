generator client {
  provider        = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model LoginGroup {
  id         Int      @id @default(autoincrement())
  name       String   @unique
  createdOn  DateTime @default(now())
}

model Login {
    id         Int      @id @default(autoincrement())
    groups     Int[]
    username   String   @unique
    password   String
    active    Boolean  @default(false)
    createdOn  DateTime @default(now())
}
