generator client {
  provider        = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserGroup {
  id         Int      @id @default(autoincrement())
  name       String
}

model User {
    uid         Int      @id @default(autoincrement())
    groups     Int[]
    username   String   @unique
    password   String
}
