{
 "extends": "../../tsconfig.json",
 "compilerOptions": {
    "moduleResolution": "nodenext",
    "lib": ["esnext"],
    "rootDirs": ["src", "./dist"],
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
    "incremental": true
 },
 "include": [
   "./src/**/*",
   "./index.ts",
   "./test/**/*"
 ],
"exclude": ["node_modules", "./test/fixtures/**/*"]
}
